# Dragon Mod - Traits
# File: common/traits/00_dragon_traits.txt
# Updated with tracks and arick_blood levels
#This is for the rewrite of the mod which turns artifact dragons into dragon characters. Rip most of everything for this from AGOT
############################################

# Standard Values
@pos_compat_high = 30
@pos_compat_medium = 15
@pos_compat_low = 5

@neg_compat_high = -30
@neg_compat_medium = -15
@neg_compat_low = -5

# Base Dragon trait
dragon = {
	name = {
		first_valid = {
			triggered_desc = {
				trigger = {
					exists = this
					has_character_flag = owned_dragon
					exists = var:current_rider
					var:current_rider ?= { has_character_flag = dragon_by_horn }
				}
				desc = trait_dragon_bound
			}
			triggered_desc = {
				trigger = {
					exists = this
					has_character_flag = owned_dragon
					exists = var:current_rider
				}
				desc = trait_dragon_tamed
			}
			triggered_desc = {
				trigger = {
					exists = this
					has_character_flag = owned_dragon
				}
				desc = trait_dragon_owned
			}
			triggered_desc = {
				trigger = {
					exists = this
					is_alive = yes
				}
				desc = trait_dragon_wild
			}
			desc = trait_dragon_generic
		}
	}
	desc = {
		first_valid = {
			triggered_desc = {
				trigger = {
					NOT = { exists = this }
				}
				desc = trait_dragon_desc
			}
			desc = trait_dragon_character_desc
		}
	}
	icon = {
		first_valid = {
			triggered_desc = {
				trigger = {
					exists = this
					has_character_flag = owned_dragon
				}
				desc = dragon_owned.dds
			}
			desc = dragon.dds
		}
	}
	category = health

	fertility = 0.0

	birth = 0
	random_creation = 0.0
	random_creation_weight = 0
	shown_in_ruler_designer = no

	#incapacitating = yes

	can_have_children = no
	disables_combat_leadership = yes
	attraction_opinion = -50
	health = 5 # Dragons are healthy in general
	life_expectancy = 150 # Dragons live 200-300 years

	flag = can_not_marry
	inheritance_blocker = all
	claim_inheritance_blocker = all

	# Should match the modifiers in dragon_flight_option
	character_travel_speed = 150
	character_travel_safety = 200

	ai_energy = -50
}

